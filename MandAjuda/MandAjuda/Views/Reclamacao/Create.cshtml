	<script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
	<script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
	<![endif]-->
</head>
<body>


	<!--================Header Area =================-->
	<header class="main_header_area">
		<div class="header_top_area">
			<div class="container">
				<div class="pull-left">
					<a href="#"><i class="fa fa-phone"></i>(XX) - XXXXX-XXXX</a>
					<a href="#"><i class="fa fa-map-marker"></i>Avenida Professor João Rodrigues - Jardim Esperança, Guaratinguetá - SP</a>
				</div>
				<div class="pull-right">
					<ul class="header_social">
						<li><a href="https://www.facebook.com/MandAjuda/?modal=admin_todo_tour"><i class="fa fa-facebook"></i></a></li>
						<li><a href="https://twitter.com/AjudaMand"><i class="fa fa-twitter"></i></a></li>
					</ul>
				</div>
			</div>
		</div>
		<div class="main_menu_area">
			<div class="container">
				<nav class="navbar navbar-default">
					<!-- Brand and toggle get grouped for better mobile display -->
					<div class="navbar-header">
						<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
							<span class="sr-only">Toggle navigation</span>
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
						</button>
						<a class="navbar-brand" href="#"><img src="~/img/MALogo.png" alt=""></a>
					</div>

					<!-- Collect the nav links, forms, and other content for toggling -->
					<div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
						<ul class="nav navbar-nav navbar-right">

							<li><a href="http://localhost:51258">Home</a></li>

							<li>@Html.ActionLink("Pesquisar", "Pesquisar", "Profissionais")</li>

							<li class="dropdown submenu active">
								<a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Chamados</a>
								<ul class="dropdown-menu">
									<li>@Html.ActionLink("Meus Chamados", "Index", "Chamados")</li>
								</ul>
							</li>

							<li>@Html.ActionLink("Sair", "LogoffCliente", "Publico")</li>

						</ul>
					</div><!-- /.navbar-collapse -->
				</nav>
			</div>
		</div>
	</header>
	<!--================Header Area =================-->

	<div class="container-fluid">
		@model MandAjuda.Models.Reclamar

		@{
			ViewBag.Title = "Create";
		}

			

		@using (Html.BeginForm())
		{
			@Html.AntiForgeryToken()
		<div class="container-fluid">
			<center><h3>Canal de reclamações</h3></center>

			<div class="col-md-6 col-sm-6">
				<img src="~/img/bonequinho para  post  vício redibitório.jpg" />
			</div>

			<div class="col-md-2 col-sm-2">
				<p></p>
			</div>
			<hr />
			@Html.ValidationSummary(true, "", new { @class = "text-danger" })
			<div class="form-group">
				@Html.LabelFor(model => model.ProfissionalId, "ProfissionalId", htmlAttributes: new { style = "display:none;", @class = "control-label col-md-2" })
				
					@Html.DropDownList("ProfissionalId", null, htmlAttributes: new { style = "display:none;", @class = "form-control" })
					@Html.ValidationMessageFor(model => model.ProfissionalId, "", new { @class = "text-danger" })
				
			</div>

			<div class="form-group">
				@Html.LabelFor(model => model.ClienteId, "ClienteId", htmlAttributes: new { style = "display:none;", @class = "control-label col-md-2" })
				
					@Html.DropDownList("ClienteId", null, htmlAttributes: new { style = "display:none;", @class = "form-control" })
					@Html.ValidationMessageFor(model => model.ClienteId, "", new { @class = "text-danger" })
				
			</div>

			<div class="form-group">
				@Html.LabelFor(model => model.From, new { style = "display:none;" }@*htmlAttributes: new { @class = "control-label col-md-2" }*@)
				
					@Html.TextBoxFor(model => model.From, new { style = "display:none;", @Value = "mandajudaservico@gmail.com", htmlAttributes = new { @class = "form-control" } })
					@Html.ValidationMessageFor(model => model.From, "", new { @class = "text-danger" })
				
			</div>

			<div class="form-group">
				@Html.LabelFor(model => model.To, new { style = "display:none;"}@*htmlAttributes: new { @class = "control-label col-md-2" }*@)
				<div>
					@Html.TextBoxFor(model => model.To, new { style = "display:none;", @Value = "pedrosantos0509@gmail.com", htmlAttributes = new { @class = "form-control" } })
					@Html.ValidationMessageFor(model => model.To, "", new { @class = "text-danger" })
				</div>
			</div>

			<div class="form-group">
				@Html.LabelFor(model => model.Subject, new { style = "display:none;" }@* htmlAttributes: new { @class = "control-label col-md-2" }*@)
				<div>
					@Html.TextBoxFor(model => model.Subject, new { style = "display:none;", @Value = "Reclamação Mand'Ajuda", htmlAttributes = new { @class = "form-control" } })

				</div>
			</div>


			<div class="form-group" style="margin-top:100px">
				@Html.LabelFor(model => model.Body, htmlAttributes: new { style = "display:none;" }@* htmlAttributes: new { @class = "control-label col-md-2" }*@)
				<div class="col-md-4">
					@Html.EditorFor(model => model.Body, new { htmlAttributes = new { @class = "form-control", style = "height:150px", placeholder = "Digite sua mensagem" } })
					@Html.ValidationMessageFor(model => model.Body, "", new { @class = "text-danger" })
				</div>
			</div>

			
				
			<input type="submit" value="Enviar" class="btn btn-warning" style="margin-top:55px" />
				
			
		</div>
		}

	</div>

	<!--================Address Area =================-->
	<section class="address_area">
		<div class="container">
			<div class="row address_inner">
				<div class="col-md-4">
					<div class="media">
						<div class="media-left">
							<img src="~/img/icon/place-icon.png" alt="">
						</div>
						<div class="media-body">
							<h4>Endereço:</h4>
							<h5>Avenida Professor João Rodrigues - Jardim Esperança, Guaratinguetá - SP</h5>
						</div>
					</div>
				</div>
				<div class="col-md-4">
					<div class="media">
						<div class="media-left">
							<img src="~/img/icon/phone-icon.png" alt="">
						</div>
						<div class="media-body">
							<h5>(XX) XXXXX-XXXX</h5>
						</div>
					</div>
				</div>
				<div class="col-md-4">
					<div class="media">
						<div class="media-left">
							<img src="~/img/icon/inbox-icon.png" alt="">
						</div>
						<div class="media-body">
							<h6 style="font-size:17px">mandajudaservico@gmail.com</h6>

						</div>
					</div>
				</div>
			</div>
		</div>
	</section>
	<!--================End Address Area =================-->
	<!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
	<script src="~/js/jquery-2.2.4.js"></script>
	<!-- Include all compiled plugins (below), or include individual files as needed -->
	<script src="~/js/bootstrap.min.js"></script>
	<!-- Rev slider js -->
	<script src="~/vendors/revolution/js/jquery.themepunch.tools.min.js"></script>
	<script src="~/vendors/revolution/js/jquery.themepunch.revolution.min.js"></script>
	<script src="~/vendors/revolution/js/extensions/revolution.extension.video.min.js"></script>
	<script src="~/vendors/revolution/js/extensions/revolution.extension.slideanims.min.js"></script>
	<script src="~/vendors/revolution/js/extensions/revolution.extension.layeranimation.min.js"></script>
	<script src="~/vendors/revolution/js/extensions/revolution.extension.navigation.min.js"></script>

	<script src="~/vendors/owl-carousel/owl.carousel.min.js"></script>
	<script src="~/vendors/isotope/imagesloaded.pkgd.min.js"></script>
	<script src="~/vendors/isotope/isotope.pkgd.min.js"></script>
	<script src="~/vendors/magnific-popup/jquery.magnific-popup.min.js"></script>
	<script src="~/vendors/counterup/waypoints.min.js"></script>
	<script src="~/vendors/counterup/jquery.counterup.min.js"></script>
	<script src="~/vendors/flex-slider/jquery.flexslider-min.js"></script>

	<!--gmaps Js-->
	<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCjCGmQ0Uq4exrzdcL6rvxywDDOvfAu6eE"></script>
	<script src="~/js/gmaps.min.js"></script>

	<script src="~/js/theme.js"></script>
</body>
</html>


